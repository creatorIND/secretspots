{
	"name": "secretspots",
	"version": "1.0.0",
	"description": "",
	"main": "app.js",
	"scripts": {
		"watch:sass": "sass sass/main.scss public/stylesheets/style.css -w",
		"compile:sass": "sass sass/main.scss public/stylesheets/style.comp.css",
		"concat:css": "concat -o public/stylesheets/style.concat.css public/stylesheets/style.comp.css",
		"prefix:css": "postcss --use autoprefixer -b 'last 10 versions' public/stylesheets/style.concat.css -o public/stylesheets/style.prefix.css",
		"compress:css": "sass public/stylesheets/style.prefix.css public/stylesheets/style.css --style=compressed",
		"build:css": "npm-run-all compile:sass concat:css prefix:css compress:css",
		"start": "node app.js",
		"test": "echo \"Error: no test specified\" && exit 1"
	},
	"keywords": [],
	"author": "Sujal Sinha",
	"license": "ISC",
	"dependencies": {
		"@mapbox/mapbox-sdk": "^0.13.0",
		"cloudinary": "^1.26.1",
		"connect-flash": "^0.1.1",
		"connect-mongo": "^4.4.1",
		"dotenv": "^10.0.0",
		"ejs": "^3.1.6",
		"ejs-mate": "^4.0.0",
		"express": "^4.17.1",
		"express-mongo-sanitize": "^2.1.0",
		"express-session": "^1.17.2",
		"helmet": "^4.6.0",
		"joi": "^17.4.0",
		"method-override": "^3.0.0",
		"mongoose": "^5.12.14",
		"multer": "^1.4.2",
		"multer-storage-cloudinary": "^4.0.0",
		"passport": "^0.7.0",
		"passport-local": "^1.0.0",
		"passport-local-mongoose": "^6.1.0",
		"sanitize-html": "^2.4.0"
	}
}
